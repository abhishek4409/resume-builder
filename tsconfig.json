{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "es6",
      "es2020",
      "ES2017",
      "ES2017.Object",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "exclude": [
    "src"
  ]
//   "include": [
//     "src"
//   ],
}
